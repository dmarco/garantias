  "config": {
    "vendor-dir": "vendor"
  },
  "require": {
    "php": ">=7.0",
    "johnpbloch/wordpress": ">=4.9",
    "roots/wp-password-bcrypt": "1.0.0",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/custom-post-type-ui": "1.5.7",
    "wpackagist-plugin/adminimize": "1.11.4",
    "deliciousbrains/wp-migrate-db-pro": "*",
    "deliciousbrains/wp-migrate-db-pro-media-files": "*",
    "deliciousbrains/wp-migrate-db-pro-cli": "*",
    "deliciousbrains/wp-offload-s3-pro": "*",
    "deliciousbrains/wp-offload-s3-assets-pull": "*",
    "wpackagist-plugin/regenerate-thumbnails": "3.0.2",
    "wpackagist-plugin/post-expirator": "2.3.1.1",
    "wpackagist-plugin/user-role-editor": "4.40.3",
    "airesvsg/acf-to-rest-api": "3.1.0",
    "codepress/admin-columns-pro": "4.2.6",
    "codepress/ac-addon-acf": "2.2.2",
    "wpackagist-plugin/wp-redis": "*",
    "wpackagist-plugin/wp-rest-api-v2-menus":"*"
    
  },
  "extra": {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/app/plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ],
      "public/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "public/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  }
}
